@model AuditLogApp.Models.Error.DescriptiveError

@{
    ViewData["Title"] = "DescriptiveError";
    Layout = "~/Views/Shared/StandaloneLayout.cshtml";
}

<div class="ala-standalone-page">
    <h2>Internal Error</h2>
    <p>
        We're sorry, an error has occurred: <br />
        <ul>
            <li>@Model.Message</li>
        </ul>
        We've been notified and will look into it right away.
    </p>
</div>